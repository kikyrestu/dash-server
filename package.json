{
  "name": "server-dashboard",
  "version": "1.0.0",
  "description": "Real-time admin dashboard for monitoring home server",
  "scripts": {
    "install-all": "cd backend && npm install && cd ../agent && npm install && cd ../frontend && npm install",
    "start-backend": "cd backend && npm start",
    "start-agent": "cd agent && npm start",
    "start-frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-agent\" \"npm run start-frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:agent": "cd agent && npm run dev",
    "dev:frontend": "cd frontend && npm start"
  },
  "keywords": ["dashboard", "monitoring", "server", "real-time", "react", "nodejs"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "ws": "^8.18.2",
    "bcrypt": "^6.0.0",
    "multer": "^2.0.1",
    "dockerode": "^4.0.7",
    "mysql2": "^3.6.0",
    "pg": "^8.11.3",
    "mongodb": "^6.1.0",
    "sqlite3": "^5.1.6",
    "redis": "^4.6.8"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "nodemon": "^3.0.0"
  }
}
